  </span><span><span class="hljs-keyword">const</span></span><span> message = </span><span><span class="hljs-variable language_">localStorage</span></span><span>.</span><span><span class="hljs-title function_">getItem</span></span><span>(</span><span><span class="hljs-string">"chat.message"</span></span><span>);

  </span><span><span class="hljs-keyword">if</span></span><span> (agent &amp;&amp; message) {
    chatName.</span><span><span class="hljs-property">innerText</span></span><span> = agent;

    chatBody.</span><span><span class="hljs-property">innerHTML</span></span><span> = <span class="hljs-string">`
      &lt;div class="mb-2"&gt;&lt;strong&gt;You:&lt;/strong&gt; </span></span><span><span class="hljs-subst">${message}</span></span><span>&lt;/div&gt;
      &lt;div class="text-center mt-4 text-warning border p-2 rounded"&gt;
        ⚠️ Avoid paying in advance! Even for delivery
      &lt;/div&gt;
    `;

    </span><span><span class="hljs-comment">// Clear localStorage if you want to reset after load</span></span><span>
    </span><span><span class="hljs-variable language_">localStorage</span></span><span>.</span><span><span class="hljs-title function_">removeItem</span></span><span>(</span><span><span class="hljs-string">"chat.agent"</span></span><span>);
    </span><span><span class="hljs-variable language_">localStorage</span></span><span>.</span><span><span class="hljs-title function_">removeItem</span></span><span>(</span><span><span class="hljs-string">"chat.message"</span></span><span>);
  }
});

    </span><span><span class="hljs-variable language_">window</span></span><span>.</span><span><span class="hljs-property">location</span></span><span>.</span><span><span class="hljs-property">href</span></span><span> = </span><span><span class="hljs-string">"messages.html"</span></span><span>;

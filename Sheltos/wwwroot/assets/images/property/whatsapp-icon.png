    </span><span><span class="hljs-variable language_">localStorage</span></span><span>.</span><span><span class="hljs-title function_">setItem</span></span><span>(</span><span><span class="hljs-string">"chat.agent"</span></span><span>, agentName);
    </span><span><span class="hljs-variable language_">localStorage</span></span><span>.</span><span><span class="hljs-title function_">setItem</span></span><span>(</span><span><span class="hljs-string">"chat.message"</span></span><span>, message);


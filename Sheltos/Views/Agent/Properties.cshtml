@using Sheltos.ViewModel
@model IEnumerable<PropertyViewModel>
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">


<!-- Mirrored from themes.pixelstrap.com/sheltos/back-end/listing.html by HTTrack Website Copier/3.x [XR&CO'2014], Sun, 15 Jun 2025 09:42:07 GMT -->
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Sheltos - Property listing page">
    <meta name="keywords" content="sheltos">
    <meta name="author" content="sheltos">
    <link rel="icon" href="~/assets1/images/favicon.png" type="image/x-icon" />
    <title>Sheltos - Property listing page</title>

    <!--Google font-->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,400i,500,500i,600,600i,700,700i,800,800i"
          rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,400i,500,500i,700,700i&amp;display=swap"
          rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Rubik:400,400i,500,500i,700,700i" rel="stylesheet">

    <!-- animate css -->
    <link rel="stylesheet" type="text/css" href="~/assets1/css/animate.css">

    <!-- magnific css -->
    <link rel="stylesheet" type="text/css" href="~/assets1/css/magnific-popup.css">

    <!-- Template css -->
    <link rel="stylesheet" type="text/css" href="~/assets1/css/admin.css">

</head>

<body>

    <!-- Loader start -->
    <div class="loader-wrapper">
        <div class="row loader-img">
            <div class="col-12">
                <img src="~/assets1/images/loader/loader-2.gif" class="img-fluid" alt="">
            </div>
        </div>
    </div>
    <!-- Loader end -->

    <div class="page-wrapper">

        <!-- page header start -->
        <div class="page-main-header row">
            <div id="sidebar-toggle" class="toggle-sidebar col-auto">
                <i data-feather="chevrons-left"></i>
            </div>
            <div class="nav-right col p-0">
                <ul class="header-menu">
                    <li>
                        <form method="get" class="mb-3">
                            <div class="input-group">
                                <input type="text" name="searchTerm" class="form-control" placeholder="Search by type..."
                                       value="@ViewBag.SearchTerm" />
                                <button class="btn btn-primary" type="submit">Search</button>
                            </div>
                        </form>

                    </li>

                    <div class="modal-dialog-centered">
                        <h4>Properties List</h2>
                    </div>
                    <li class="onhover-dropdown notification-box">
                       
                        <div class="notification-dropdown onhover-show-div">
                            <div class="dropdown-title">
                                <h6>Notifications</h6>
                                <a href="favourites.html">Show all</a>
                            </div>
                            <ul>


                                <li>
                                    <div class="media">
                                        <div class="icon-notification bg-warning-light">
                                            <i class="fas fa-comment-dots"></i>
                                        </div>
                                        <div class="media-body">
                                            <h6>New Inquiry</h6>
                                            <span>1 Days ago, Krowada 7</span>
                                            <p class="mb-0">"is the villa still available?"</p>
                                            <ul class="user-group">
                                                <li>
                                                    <a href="javascript:void(0)">
                                                        <img src="~/assets1/images/about/2.jpg" class="img-fluid" alt="">
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="javascript:void(0)">
                                                        <img src="~/assets1/images/about/3.jpg" class="img-fluid" alt="">
                                                    </a>
                                                </li>
                                                <li class="reply">
                                                    <a href="javascript:void(0)">
                                                        Reply
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </li>
                    <li class="profile-avatar onhover-dropdown">
                        <div>
                            <img src="~/assets1/images/avatar/3.jpg" class="img-fluid" alt="">
                        </div>
                        <ul class="profile-dropdown onhover-show-div">
                            <li><a asp-action="Properties" asp-controller="Agent"><span>Listing </span><i data-feather="user"></i></a></li>

                            <li><a asp-controller="Controller" asp-action="Login"><span>Log Out</span><i data-feather="log-out"></i></a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>

        <!-- page header end -->
        <div class="page-body-wrapper">

            <!-- page sidebar start -->
            <div class="page-sidebar">
                <div class="logo-wrap">
                    <a href="index.html">
                        <img src="~/assets1/images/logo/4.png" class="img-fluid for-light" alt="">
                        <img src="~/assets1/images/logo/9.png" class="img-fluid for-dark" alt="">
                    </a>
                    <div class="back-btn d-lg-none d-inline-block">
                        <i data-feather="chevrons-left"></i>
                    </div>
                </div>
                <div class="main-sidebar">
                    <div class="user-profile">
                        <div class="media">
                            <div class="change-pic">
                                <img src="~/assets1/images/avatar/3.jpg" class="img-fluid" alt="">
                            </div>
                            <div class="media-body">
                                <h4>Agent</h4>

                            </div>
                        </div>
                    </div>
                    <div id="mainsidebar">
                        <ul class="sidebar-menu custom-scrollbar">
                            <li class="sidebar-item">
                                <a asp-action="DashBoard" asp-controller="Agent" class="sidebar-link only-link">
                                    <i data-feather="airplay"></i>
                                    <span>Dashboard</span>
                                </a>
                            </li>
                            <li class="sidebar-item">
                                <a href="javascript:void(0)" class="sidebar-link">
                                    <i data-feather="grid"></i>
                                    <span>Properties</span>
                                </a>
                                <ul class="nav-submenu menu-content">
                                    <li>
                                        <a asp-action="AddProperty" asp-controller="Agent">
                                            <i data-feather="chevrons-right"></i>
                                            add property
                                        </a>
                                    </li>

                                    <li>
                                        <a asp-action="Properties" asp-controller="Agent">
                                            <i data-feather="chevrons-right"></i>
                                            Property List
                                        </a>
                                    </li>

                                </ul>
                            </li>
                            <li class="sidebar-item">
                                <a asp-controller="Agent" asp-action="Message" class="sidebar-link only-link">
                                    <i data-feather="users"></i>
                                    <span>Property Request</span>
                                </a>
                            </li>

                            <li class="sidebar-item">
                                <a href="javascript:void(0)" class="sidebar-link">
                                    <i data-feather="unlock"></i>
                                    <span>Authentication</span>
                                </a>
                                <ul class="nav-submenu menu-content">
                                    <li>
                                        <a asp-action="Login" asp-controller="Account">
                                            <i data-feather="chevrons-right"></i>
                                            LogOut
                                        </a>
                                    </li>
                                   

                                </ul>
                            </li>
                            <li>
                                <div class="upgrade-box">
                                    <img src="https://themes.pixelstrap.com/sheltos/assets/images/svg/1.svg" class="img-fluid" alt="">
                                    <h6>Need Help</h6>
                                    <a href="https://pixelstrap.freshdesk.com/support/home" target="_blank" class="p-0 m-0">
                                        <span class="d-block">Raise ticket at "support@pixelstrap.com"
                                    </a>
                                    </a>
                                    <button type="button" onclick=" window.open('https://themeforest.net/user/pixelstrap/portfolio', '_blank');" class="btn btn-pill btn-gradient color-4 btn-sm mt-2 fw-bold">Buy Now</button>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- page sidebar end -->
            <div class="page-body">



                <!-- Container-fluid start -->
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="property-admin">
                                <div class="property-section section-sm">
                                    <div class="row ratio_55 property-grid-2 property-map map-with-back">

                                        <div class="col-xl-12">
                                            <div class="property-2 row column-sm property-label property-grid">
                                                @if(Model != null && Model.Any())
                                                {
                                                    @foreach (var item in Model)
                                                    {
                                                        <div class="col-xl-4 col-md-6 xl-6">
                                                            <div class="property-box">
                                                                <div class="property-image">
                                                                    <div class="property-slider">
                                                                        @foreach (var image in item.GalleryImages)
                                                                        {
                                                                            <a href="javascript:void(0)">
                                                                                <img src="@image" class="bg-img" alt="Propertyimage">
                                                                            </a>

                                                                        }
                                                                    </div>
                                                                    <div class="labels-left">
                                                                        <div>
                                                                            <span class="label label-shadow">@item.PropertyStatus</span>
                                                                        </div>
                                                                    </div>

                                                                </div>

                                                                <div class="property-details">
                                                                    <span class="font-roboto">@($"{item.City}, {item.State}, {item.Country}")</span>
                                                                    <a href="https://themes.pixelstrap.com/sheltos/main/single-property-8.html">
                                                                        <h3>@item.Title</h3>
                                                                    </a>
                                                                    <h6>@item.Price.ToString("C")</h6>
                                                                    <p class="font-roboto light-font">@item.Description</p>
                                                                    <ul>
                                                                        <li><img src="https://themes.pixelstrap.com/sheltos/assets/images/svg/icon/double-bed.svg" class="img-fluid" alt="">Bed : @item.Beds</li>
                                                                        <li><img src="https://themes.pixelstrap.com/sheltos/assets/images/svg/icon/bathroom.svg" class="img-fluid" alt="">Baths : @item.Bathrooms</li>
                                                                        <li><img src="https://themes.pixelstrap.com/sheltos/assets/images/svg/icon/square-ruler-tool.svg" class="img-fluid ruler-tool" alt="">Sq Ft : @item.PropertySize</li>
                                                                    </ul>
                                                                    <span>@item.DateTime</span>
                                                                    <div class="property-btn d-flex">
                                                                        <div>
                                                                            <a asp-action="DeleteProperty" asp-controller="Agent" class="btn btn-dashed btn-pill color-2" asp-route-id="@item.Id" onsubmit="return confirm('Are you sure you want to delete this property?');">Delete</a>
                                                                            <a asp-controller="Property" asp-action="Details" asp-route-id="@item.Id" class="btn btn-secondary">Details</a>
                                                                            <a asp-action="Edit" asp-controller="Agent" asp-route-id="@item.Id" class="btn btn-dashed btn-pill color-2">Edit</a>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    }
                                                }
                                                else
                                                {
                                                    <div class="col-12 text-center mt-5">
                                                        <h4>No Available Properties</h4>
                                                    </div>
                                                }
                                            </div>
                                            <nav class="theme-pagination">
                                                <ul class="pagination">
                                                    <li class="page-item">
                                                        <a class="page-link" href="javascript:void(0)" aria-label="Previous">
                                                            <span aria-hidden="true">«</span>
                                                            <span class="sr-only">Previous</span>
                                                        </a>
                                                    </li>
                                                    <li class="page-item active"><a class="page-link" href="javascript:void(0)">1</a></li>
                                                    <li class="page-item"><a class="page-link" href="javascript:void(0)">2</a></li>
                                                    <li class="page-item"><a class="page-link" href="javascript:void(0)">3</a></li>
                                                    <li class="page-item">
                                                        <a class="page-link" href="javascript:void(0)" aria-label="Next">
                                                            <span aria-hidden="true">»</span>
                                                            <span class="sr-only">Next</span>
                                                        </a>
                                                    </li>
                                                </ul>
                                            </nav>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Container-fluid end -->
            </div>




            <!-- footer start -->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-6 footer-copyright">
                            <p class="mb-0">Copyright 2022 © Sheltos All rights reserved.</p>
                        </div>
                        <div class="col-md-6">
                            <p class="float-end mb-0">Developed with  <i class="fa fa-heart font-danger"></i></p>
                        </div>
                    </div>
                </div>
            </footer>
            <!-- footer end -->
        </div>
    </div>

    <!-- tap to top start -->
    <div class="tap-top">
        <div>
            <i class="fas fa-arrow-up"></i>
        </div>
    </div>
    <!-- tap to top end -->
    <!-- customizer start -->
    <div class="customizer-wrap">
        <div class="customizer-links">
            <i data-feather="settings"></i>
        </div>
        <div class="customizer-contain custom-scrollbar">
            <div class="setting-back">
                <i data-feather="x"></i>
            </div>
            <div class="layouts-settings">
                <div class="customizer-title">
                    <h6 class="color-4">Layout type</h6>
                </div>
                <div class="option-setting">
                    <span>Light</span>
                    <label class="switch">
                        <input type="checkbox" name="chk1" value="option" class="setting-check"><span class="switch-state"></span>
                    </label>
                    <span>Dark</span>
                </div>
            </div>
            <div class="layouts-settings">
                <div class="customizer-title">
                    <h6 class="color-4">Layout Direction</h6>
                </div>
                <div class="option-setting">
                    <span>LTR</span>
                    <label class="switch">
                        <input type="checkbox" name="chk2" value="option" class="setting-check1"><span class="switch-state"></span>
                    </label>
                    <span>RTL</span>
                </div>
            </div>
            <div class="layouts-settings">
                <div class="customizer-title">
                    <h6 class="color-4">Unlimited Color</h6>
                </div>
                <div class="option-setting unlimited-color-layout">
                    <div class="form-group">
                        <label for="ColorPicker6">color 6</label>
                        <input id="ColorPicker6" type="color" value="#f35d43" name="Default">
                    </div>
                    <div class="form-group">
                        <label for="ColorPicker7">color 7</label>
                        <input id="ColorPicker7" type="color" value="#f34451" name="Default">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- customizer end -->
    <!-- latest jquery-->
    <script src="~/assets1/js/jquery-3.6.0.min.js"></script>


    <!-- Bootstrap js-->
    <script src="~/assets1/js/bootstrap.bundle.min.js"></script>

    <!-- feather icon js-->
    <script src="~/assets1/js/feather-icon/feather.min.js"></script>
    <script src="~/assets1/js/feather-icon/feather-icon.js"></script>

    <!-- magnific js -->
    <script src="~/assets1/js/jquery.magnific-popup.js"></script>
    <script src="~/assets1/js/zoom-gallery.js"></script>

    <!-- sidebar js -->
    <script src="~/assets1/js/sidebar.js"></script>

    <!-- slick js -->
    <script src="~/assets1/js/slick.js"></script>
    <script src="~/assets1/js/slick-animation.min.js"></script>
    <script src="~/assets1/js/custom-slick.js"></script>

    <!--admin js -->
    <script src="~/assets1/js/admin-script.js"></script>

    <!-- Customizer js-->
    <script src="~/assets1/js/customizer.js"></script>

    <!-- Color-picker js-->
    <script src="~/assets1/js/color/custom-colorpicker.js"></script>

</body>



    <script>
        function downloadPropertyViews(propertyId) {
        const data = localStorage.getItem(`views-${propertyId}`);
        if (!data) return alert("No views yet.");

        const blob = new Blob([data], { type: "application/json" });
        const url = URL.createObjectURL(blob);

        const a = document.createElement("a");
        a.href = url;
        a.download = `${propertyId}-views.json`;
        a.click();
        }
    </script>
